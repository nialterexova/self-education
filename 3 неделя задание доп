#include <math.h>
#include <stdio.h>
#include <stdlib.h>
 
int main() 
{int a,b,c;
scanf("%d%d%d",&a,&b,&c);
if (((a>12)&&(b>12))||(a==0)||(b==0))
    printf ("некорректные числа\n");
else 
    {if ((a<13)&&(b>12)&&(b<32))
        printf ("1");
   else if ((13<=a)&&(a<=31)&&(b<=12))
       printf("1");
    else 
    {printf ("0");
    return 0;
    }
    }
}
